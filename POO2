class Animal:
  def __init__(self, nome):
    self.nome = nome
    def emitir_som(self):
      print("Som gen√©rico...")

class  Cachorro(Animal):
   def emitir_som(self):
     print("Au au!")

class Gato(Animal):
   def emitir_som(self):
     print("Miau!")

dog = Cachorro("Rex")
cat = Gato("Mimi")

dog.emitir_som() #auau
cat.emitir_som() #miau